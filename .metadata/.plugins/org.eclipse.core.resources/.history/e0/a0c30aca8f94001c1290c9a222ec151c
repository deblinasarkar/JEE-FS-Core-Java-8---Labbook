package com.cg.feb22.demo;

public class prime {
public static void main(String[] args) {
	int i,j,c=0;
	for(i=2;i<=100;i++)
	{
		for(j=2;j<=i/2;i++)
        {
            if(i%j==0)
            {
                c++;
            }
        }
            if(c==0)
                System.out.println("prime numbers are "+ i);
        }
        
            System.out.println("not prime");
	}
}
}
