package com.cg.feb22.oop.intro;

public class Customer {
	int eid;
	String firstName;
	double salary;
	
	public Customer() {
		super();
	}

	public Customer(int eid, String firstName) {
		super();
		this.eid = eid;
		this.firstName = firstName;
	}

	public Customer(int eid, String firstName, double salary) {
		super();
		this.eid = eid;
		this.firstName = firstName;
		this.salary = salary;
	}

	void work() {
		System.out.println(firstName + " works...");
	}

	@Override
	public String toString() {
		return "Customer [eid=" + eid + ", firstName=" + firstName + ", salary=" + salary + "]";
	}
}
