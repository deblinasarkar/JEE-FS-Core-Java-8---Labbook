package lab2;
import java.util.*;

public class ex4 {
	public static void main(String[] args) {
		int n;
		Scanner sc=new Scanner(System.in);
		n=sc.nextInt();
		int[] a=new int[n];
		for(int i=0;i<n;i++)
		{
			a[i]=sc.nextInt();
		}
		int[] k=ex4.modifyArray(a);
		for(int g=0;g<n;g++)
		{
			System.out.println(k[g]);
		}
	}
	public static int[] modifyArray(int[] a)
	{
		int i,j=0,p=0;
		int[] temp=new int[100];
		int[] h=new int[100];
		for(i=0;i<a.length-1;i++)
		{
			if(a[i]!=a[i+1])
			{
				temp[j++]=a[i];
				//temp[j++]=a[a.length-1];
			}
				
		}
		//if(temp[j]!=temp[a.length-1])
		//{
			//temp[j]=
		//}
		for(int l=0;l<=j;l++)
		{
			h[p++]=temp[l];
		}
		return h;
		
	}
}
 