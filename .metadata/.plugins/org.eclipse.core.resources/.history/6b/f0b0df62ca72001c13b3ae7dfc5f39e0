package co.deblina;
import java.util.*;
public class toysort {
 
	public static void main(String[] args)
	{
		String[][] arr=new String[5][3];
		int i,j;
		Scanner sc=new Scanner(System.in);
		try {
		for(i=0;i<=5;i++)
		{
			for(j=0;j<=3;j++)
			{
				arr[i][j]=sc.nextLine();
			}
		}
		
		sort(arr);
		
		for(i=0;i<=5;i++)
		{
			for(j=0;j<=3;j++)
			{
				System.out.println(arr[i][j]);
			}
		}
		}
		catch(Exception e)
		{
			
		}
		
	}
	
	public static void sort(String arr[][])
	{
		String id,nm,ct;
		boolean sorted=false;
		while(!sorted)
		{
			sorted=true;
			for(int i=0;i<=5;i++)
			{
				if(Integer.parseInt(arr[i][0])<Integer.parseInt(arr[i+1][0]))
						{
							sorted=false;
							id=arr[i][0];
							arr[i][0]=arr[i+1][0];
							arr[i+1][0]=id;
							
							nm=arr[i][1];
							arr[i][1]=arr[i+1][1];
							arr[i+1][1]=nm;
							
							ct=arr[i][2];
							arr[i][2]=arr[i+1][2];
							arr[i+1][2]=ct;
							
						}
			}
			
		}
	}
	
}
