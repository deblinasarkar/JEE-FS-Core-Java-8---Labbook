package com.cg.feb22.demo;

public class prime {
public static void main(String[] args) {
	int i,j;
	for(i=2;i<=100;i++)
	{
		int c=0;
		for(j=1;j<=i;i++)
        {
            if(i%j==0)
            {
                c++;
               // break;
            }
            if(c==2)
                System.out.println("prime numbers are "+ i);
            c=0;
        }
        
            System.out.println("not prime");
	}
}
}
