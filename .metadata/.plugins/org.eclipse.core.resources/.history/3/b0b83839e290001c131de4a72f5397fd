import java.util.*;
public class highestfeedback {
	public static void main(String[] args)
	{
	int n;
	
	Scanner sc=new Scanner(System.in);
	n=sc.nextInt();
	int[] a1=new int[n];
	int[] a2=new int[n];
	int i,j;
	if(n<0)
	{
		System.out.println("Invalid array size");
	}
	else 
	{
		for(i=0;i<n;i++)
		{
			a1[i]=sc.nextInt();
			if(a1[i]<0)
			{
				break;
			}
		}
		for(j=0;j<n;j++)
		{
			a2[j]=sc.nextInt();
			if(a2[j]<0)
			{
				break;
			}
		}
	}
	
		highestfeedback(a1,a2,n);

	
	
	
	}
	public static void highestfeedback(int[] a1,int[] a2,int n)
	{
		int i,j;
		int b[]=new int[n];
		for(i=0;i<n;i++)
		{
			if(a1[i]==a2[i])
			{
				if(a1[i+1]>a2[i+i])
				{
					b[i]=a1[i];
					b[i+1]=a1[i+i];
				}
				else
				{
					b[i]=a1[i];
					b[i+1]=a2[i+i];
				}
			}
		}
		for(i=0;i<b.length;i++)
		{
			System.out.println(i);
		}
	}
}
