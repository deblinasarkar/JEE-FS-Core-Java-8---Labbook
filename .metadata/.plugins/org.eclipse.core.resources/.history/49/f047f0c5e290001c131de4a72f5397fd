import java.util.*;
public class highestfeedback {
	public static void main(String[] args)
	{
	int n;
	
	Scanner sc=new Scanner(System.in);
	n=sc.nextInt();
	int[] a1=new int[n];
	int[] a2=new int[n];
	int i,j;
	if(n<0)
	{
		System.out.println("Invalid array size");
	}
	else 
	{
		for(i=0;i<n;i++)
		{
			a1[i]=sc.nextInt();
			if(a1[i]<0)
			{
				System.out.println("Invalid Input");
				System.exit(0);
			}
		}
		for(j=0;j<n;j++)
		{
			a2[j]=sc.nextInt();
			if(a2[j]<0)
			{
				System.out.println("Invalid Input");
				System.exit(0);
			}
		}
	}
	
		highestfeedback(a1,a2,n);

	
	
	
	}
	public static void highestfeedback(int[] a1,int[] a2,int n)
	{
		int i,j,c=0,d=1;
		int b[]=new int[n];
		for(i=0;i<n;i+2)
		{
			for(j=0;j<n;j+2)
			{
				if(a1[i]==a2[j])
				{
					b[c]=a1[i];
					if(a1[i+1]>a2[j+1])
					{
						b[c+1]=a1[i+1];
						
					}
					else
					{
						b[c+1]=a2[j+1];
					}
					c++;
				}
			}
		}
		for(i=0;i<b.length;i++)
		{
			System.out.println(i);
		}
	}
}
