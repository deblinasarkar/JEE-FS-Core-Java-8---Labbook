package lab2;
import java.util.*;

public class ex4 {
	public static void main(String[] args) {
		int n;
		Scanner sc=new Scanner(System.in);
		n=sc.nextInt();
		int[] a=new int[n];
		for(int i=0;i<n;i++)
		{
			a[i]=sc.nextInt();
		}
		int[] k=ex4.modifyArray(a);
		for(int g=0;g<k;g++)
		{
			System.out.println(k[g]);
		}
	}
	public static int[] modifyArray(int[] a)
	{
		int i,k=0,p=0;
		int l=a.length;
		int[] temp = new int[l];  
        int j = 0;  
        for (i=0; i<l-1; i++){  
            if (a[i]!=a[i+1]){  
                temp[j++] = a[i];  
            }  
         }  
        temp[j++] = a[l-1];     
        // Changing original array  
        for (i=0; i<j; i++){  
            a[i] = temp[i];  
        }  
        return a;  
    }  
}
 