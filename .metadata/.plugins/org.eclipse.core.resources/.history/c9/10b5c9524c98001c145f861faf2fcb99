package com.cg.feb22.misc;
import java.util.regex.Matcher;
import java.util.regex.Pattern;

public class RegexDemo {
	public static void main(String[] args) {
		String input="abcd";
		String regex="a";
		Pattern pat=Pattern.compile(regex); //static method compile
		Matcher mat=pat.matcher(input); // non static bcz its pat object
		System.out.println(mat.matches());
		System.out.println(mat.find());
		System.out.println(mat.lookingAt());
	}
}

/*matches method is like equal method
find to check whether a part of string is present or not
lookingat used to check the input sequence at the beginning*/